<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginLeft="5dp"
              android:layout_marginRight="5dp"
              android:orientation="horizontal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp">

        <include layout="@layout/view_line_v_m"/>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/date"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/normal_invest_success_money"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/normal_invest_success_count"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/normal_invest_fail_money"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/normal_invest_fail_count"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/vip_invest_success_money"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/vip_invest_success_count"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/vip_invest_fail_money"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/vip_invest_fail_count"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/success_money"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/success_count"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/fail_money"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:text="@string/fail_count"
                android:textColor="@color/txt_color2"
                android:textSize="12dp">
            </TextView>

            <include layout="@layout/view_line_h"/>
        </LinearLayout>

        <include layout="@layout/view_line_v_m"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_date"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_normal_invest_success_money"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_normal_invest_success_count"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_normal_invest_fail_money"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_normal_invest_fail_count"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_vip_invest_success_money"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_vip_invest_success_count"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_vip_invest_fail_money"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_vip_invest_fail_count"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_success_money"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_success_count"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_fail_money"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

            <TextView
                android:id="@+id/tv_fail_count"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:textColor="@color/txt_color1"
                android:textSize="14dp">
            </TextView>

            <include layout="@layout/view_line_h"/>

        </LinearLayout>

        <include layout="@layout/view_line_v_m"/>
    </LinearLayout>

</LinearLayout>

